{
    "name": "ask-clock",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "core-js": "^2.6.5",
        "d3": "^5.9.2",
        "es6-promise": "^4.2.8",
        "moment": "^2.24.0",
        "moment-hijri": "^2.1.2",
        "vue": "^2.6.10",
        "vue-router": "^3.0.3",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.8.0",
        "@vue/cli-plugin-eslint": "^3.8.0",
        "@vue/cli-service": "^3.8.0",
        "@vue/eslint-config-standard": "^4.0.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "mockjs": "^1.0.1-beta3",
        "sass": "^1.18.0",
        "sass-loader": "^7.1.0",
        "vue-template-compiler": "^2.6.10"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "@vue/standard"
        ],
        "rules": {
            "indent": [
                2,
                4,
                {
                    "ArrayExpression": "off",
                    "ObjectExpression": "off"
                }
            ],
            "semi": [
                2,
                "always"
            ],
            "no-trailing-spaces": [
                2,
                {
                    "skipBlankLines": true
                }
            ],
            "space-before-function-paren": [
                2,
                "never"
            ],
            "no-multiple-empty-lines": [
                2,
                {
                    "max": 2
                }
            ],
            "prefer-promise-reject-errors": [
                0
            ],
            "vue/valid-v-on": "off",
            "no-extend-native": ["error", {
                "exceptions": ["String", "Number"]
            }]
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
